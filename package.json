{
  "name": "@evokedresponse/react-native-material-kit",
  "version": "1.0.5",
  "description": "Bringing Material Design to React Native",
  "main": "index.js",
  "typings": "index.d.ts",
  "author": {
    "name": "Evoked Response",
    "url": "https://www.github.com/EvokedResponse"
  },
  "contributors": [
    "Yingxin Wu (https://github.com/xinthink)",
    "Quentin Valmori (https://github.com/Crash--)",
    "Alexander Price (https://github.com/alexprice91)"
  ],
  "scripts": {
    "lint": "tslint -p .",
    "test": "jest",
    "emit-js": "yarn lint && tsc",
    "update-version": "./scripts/updateVersion.sh",
    "build-docs": "yarn clean && yarn setup && yarn ln && yarn emit-js && api-extractor run --local --verbose && yarn api-md",
    "clean": "rm -rf lib temp etc dist docs && yarn clean-ln",
    "clean-ln": "rm -f src/mdl/Spinner.tsx",
    "ln": "ln -f src/mdl/Spinner.ios.tsx src/mdl/Spinner.tsx",
    "build-clean": "yarn build-docs && yarn clean-ln && rm lib/mdl/Spinner.js",
    "build": "tsc",
    "package": "cd dist && npm publish",
    "setup": "mkdir temp etc dist docs",
    "api-md": "api-documenter markdown --input-folder temp --output-folder docs/api"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EvokedResponse/react-native-material-kit.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.3.11",
    "@microsoft/api-extractor": "^7.3.4",
    "@react-native-community/eslint-config": "~0.0.7",
    "@types/jest": "^25.1.0",
    "@types/ramda": "^0.26.0",
    "@types/react-native": "^0.57.7",
    "@types/react-test-renderer": "^16.9.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.12.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "ramda": "^0.27.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.50.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ]
  }
}
